package duy.main;
/*
* @description:
* @author: Chopper
* @date: Nov 26, 2023
* @version: 1.0
*/

/**
 * 
 */
public class Test {
public static int switchIt(int x)
{
int j = 1;
switch (x) {
case 1:
j++;
case 2:
j++;
case 3:
j++;
case 4:
j++;
default:
j++;
}
return j + x;
}
public static void main(String[] args) {
System.out.println("value = " + switchIt(4));
}
}
